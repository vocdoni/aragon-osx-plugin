{
  "name": "@vocdoni/aragon-osx-plugin",
  "version": "0.1",
  "description": "Vocdoni off-chain voting plugin for Aragon OSx",
  "main": "dist/bundle-cjs.js",
  "module": "dist/bundle-esm.js",
  "types": "dist/bundle.d.ts",
  "files": [
    "dist"
  ],
  "repository": "https://github.com/vocdoni/aragon-osx-plugin",
  "author": "Vocdoni",
  "license": "AGPL-3.0-or-later",
  "scripts": {
    "test": "npx hardhat test",
    "build": "npx hardhat compile",
    "build:npm": "rollup --config rollup.config.ts",
    "coverage": "npx hardhat coverage --solcoverjs ./.solcover.js",
    "flatten": "npx hardhat flatten",
    "analyze": "mythx analyze",
    "deploy": "npx hardhat deploy",
    "dev": "yarn npx hardhat node --hostname 0.0.0.0",
    "prepublishOnly": "yarn build && yarn build:npm",
    "docgen": "npx hardhat docgen",
    "formatting:check": "prettier '**/*.{json,sol,ts,js,md}' -c",
    "formatting:write": "prettier '**/*.{json,sol,ts,js,md}' --write",
    "clean": "rm -rf artifacts cache deployments typechain"
  },
  "devDependencies": {
    "@aragon/osx": "^1.2.0",
    "@defi-wonderland/smock": "^2.3.4",
    "@ensdomains/ens-contracts": "0.0.11",
    "@nomicfoundation/hardhat-chai-matchers": "^1.0.5",
    "@nomicfoundation/hardhat-toolbox": "2.0.2",
    "@nomiclabs/hardhat-ethers": "^2.2.1",
    "@nomiclabs/hardhat-etherscan": "^3.1.4",
    "@opengsn/contracts": "2.2.6",
    "@openzeppelin/contracts": "4.8.1",
    "@openzeppelin/contracts-upgradeable": "4.8.1",
    "@openzeppelin/hardhat-upgrades": "^1.23.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-typescript": "^8.3.1",
    "@typechain/ethers-v5": "^7.2.0",
    "@typechain/hardhat": "^2.3.1",
    "@types/chai": "^4.2.22",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.11.7",
    "chai": "^4.3.4",
    "dotenv": "^10.0.0",
    "ethereumjs-util": "^7.1.4",
    "ethers": "^5.5.1",
    "hardhat": "^2.12.7",
    "hardhat-deploy": "^0.9.26",
    "hardhat-gas-reporter": "^1.0.4",
    "ipfs-http-client": "51.0.0",
    "prettier": "^2.4.1",
    "prettier-plugin-solidity": "^1.1.1",
    "rollup": "^2.70.1",
    "rollup-plugin-dts": "^4.2.0",
    "solhint": "^3.3.6",
    "solidity-coverage": "^0.8.2",
    "solidity-docgen": "^0.6.0-beta.35",
    "tmp-promise": "^3.0.3",
    "ts-node": "^8.1.0",
    "typechain": "^5.2.0",
    "typescript": "^4.4.4"
  }
}
